html
  head
    link(rel='stylesheet', href='terminal.css')
    script(src='https://code.jquery.com/jquery-3.3.1.min.js')
    script(src='https://unpkg.com/jquery.terminal@1.x.x/js/jquery.terminal.min.js')
    link(href='https://unpkg.com/jquery.terminal@1.x.x/css/jquery.terminal.min.css')
    script(src='js/jquery.mousewheel-min.js')
    title Login Here
  body.container
    .page-header
      h1 My City
    nav.navbar.navbar-default
      .container-fluid
        // Collect the nav links, forms, and other content for toggling
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
          ul.nav.navbar-nav
            li
              a(href='/') Home
            li
              a(href='/signup') Sign Up
            li
              a(href='/dashboard') Dashboard
          ul.nav.navbar-nav.navbar-right
            li
              a(href='/login') Log In
            li
              a(href='/logout') Log Out
        // /.navbar-collapse
        // /.container-fluid
    .container.row
      h1 Hi, Welcome to #{user.username} Dashboard
    #terminal
      script.
        (function($){
        $('#terminal').terminal(function(command, term) {
        if (command !== '') {
        try {
        var result = window.eval(command);
        if (result !== undefined)
        term.echo(new String(result));
        } catch(e) {
        term.error(new String(e));
        }
        } else
        term.echo('');
        },
        {
        name: 'js_demo',
        height: 1000,
        width:1500,
        prompt: 'js> '
        });
        })(jQuery);
